{
  "name": "@danny.andrews/eslint-config",
  "version": "6.0.0",
  "description": "The eslint config I use for personal projects",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Danny Andrews",
  "files": [
    "rulesets",
    "*.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danny-andrews/eslint-config.git"
  },
  "scripts": {
    "ci": "yarn lint && yarn test",
    "lint": "eslint . && sort-package-json",
    "test": "ava"
  },
  "dependencies": {
    "eslint-config-eslint": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^4.1.1",
    "ramda": "^0.24.1",
    "sort-package-json": "^1.7.0"
  },
  "peerDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.0.0"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "tests/**/*-test.js"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "failFast": false
  }
}
