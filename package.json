{
  "name": "@danny.andrews/eslint-config",
  "version": "1.0.0",
  "description": "The eslint config I use for personal projects",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "author": "Danny Andrews",
  "files": [
    "eslint-rules",
    "*.js"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danny-andrews/eslint-config.git"
  },
  "scripts": {
    "lint": "eslint -c index.js ./*.js && node_modules/.bin/sort-package-json",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1",
    "sort-package-json": "^1.4.0"
  },
  "peerDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.14.1"
  },
  "ava": {
    "files": [
      "tests/**/*-test.js"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "failFast": false
  }
}
